<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">oracle.jdbc.OracleDriver</property>
        <property name="hibernate.dialect">org.hibernate.dialect.Oracle12cDialect</property>
        <property name="show_sql">false</property>
        <property name="hibernate.temp.use_jdbc_metadata_defaults">false</property>
        <property name="hibernate.proc.param_null_passing">true</property>

        <mapping class="com.tcbs.automation.isquare.UmRoleEntity"/>
        <mapping class="com.tcbs.automation.isquare.UmUserRoleEntity"/>
        <mapping class="com.tcbs.automation.isquare.UmUserEntity"/>
        <mapping class="com.tcbs.automation.isquare.CompanyEntity"/>
        <mapping class="com.tcbs.automation.isquare.UmUserAttributeEntity"/>
        <mapping class="com.tcbs.automation.isquare.IsAuthenInfoEntity"/>

    </session-factory>
</hibernate-configuration>
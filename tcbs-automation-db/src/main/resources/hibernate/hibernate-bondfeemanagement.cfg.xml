<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
        <property name="hibernate.dialect">org.hibernate.dialect.Oracle12cDialect</property>
        <property name="show_sql">false</property>
        <property name="hibernate.temp.use_jdbc_metadata_defaults">false</property>
        <property name="hibernate.proc.param_null_passing">true</property>

        <mapping class="com.tcbs.automation.bondfeemanagement.CategoryType"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.ReferenceData"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.bond.BondBaseInfo"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.bondgroup.BondGroup"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.bondgroup.BondGroupMapping"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.ApprovalHistory"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.fee.BondFee"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.fee.BondFeeTerm"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.bond.BondContractDate"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.bond.BondContractPaymentDate"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.bond.BondTemplate"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.bond.CaseInfo"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.FeeTimelineEngineInstance"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.FeeTimeline"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.FeeTimelineTerm"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.FeeEventExtend"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.FeeEventPayment"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.FeeTimelineEvent"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.BpmProcessTask"/>
        <mapping class="com.tcbs.automation.bondfeemanagement.entity.event.EventDocument"/>
    </session-factory>
</hibernate-configuration>
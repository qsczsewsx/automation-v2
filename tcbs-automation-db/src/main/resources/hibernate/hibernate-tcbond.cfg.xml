<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
        <property name="hibernate.dialect">org.hibernate.dialect.Oracle12cDialect</property>
        <property name="show_sql">false</property>
        <property name="hibernate.temp.use_jdbc_metadata_defaults">false</property>
        <property name="hibernate.proc.param_null_passing">true</property>

        <mapping class="com.tcbs.automation.tcbond.InvPlanData"/>
        <mapping class="com.tcbs.automation.tcbond.InvCbfPaymentList"/>
        <mapping class="com.tcbs.automation.tcbond.BondCategory"/>
        <mapping class="com.tcbs.automation.tcbond.Customer"/>
        <mapping class="com.tcbs.automation.tcbond.Issuer"/>
        <mapping class="com.tcbs.automation.tcbond.Bond"/>
        <mapping class="com.tcbs.automation.tcbond.Coupon"/>
        <mapping class="com.tcbs.automation.tcbond.InvOrderAttr"/>
        <mapping class="com.tcbs.automation.tcbond.Trading"/>
        <mapping class="com.tcbs.automation.tcbond.Holiday"/>
        <mapping class="com.tcbs.automation.tcbond.OrderMarket"/>
        <mapping class="com.tcbs.automation.tcbond.OrderMatched"/>
        <mapping class="com.tcbs.automation.tcbond.TradingSellIconnect"/>
        <mapping class="com.tcbs.automation.tcbond.Intradaylimit"/>
        <mapping class="com.tcbs.automation.tcbond.Rm"/>
        <mapping class="com.tcbs.automation.tcbond.BondProduct"/>
        <mapping class="com.tcbs.automation.tcbond.InvBondYieldToMaturity"/>
        <mapping class="com.tcbs.automation.tcbond.Namevaluestorage"/>
    </session-factory>
</hibernate-configuration>
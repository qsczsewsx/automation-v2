<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
        <property name="hibernate.dialect">org.hibernate.dialect.Oracle12cDialect</property>
        <property name="show_sql">false</property>
        <property name="hibernate.temp.use_jdbc_metadata_defaults">false</property>
        <property name="hibernate.proc.param_null_passing">true</property>

        <mapping class="com.tcbs.automation.docportal.CategoryType"/>
        <mapping class="com.tcbs.automation.docportal.Company"/>
        <mapping class="com.tcbs.automation.docportal.DraftObject"/>
        <mapping class="com.tcbs.automation.docportal.FileBaseInfo"/>
        <mapping class="com.tcbs.automation.docportal.FileHistory"/>
        <mapping class="com.tcbs.automation.docportal.FileMetadata"/>
        <mapping class="com.tcbs.automation.docportal.FileOwner"/>
        <mapping class="com.tcbs.automation.docportal.FileRole"/>
        <mapping class="com.tcbs.automation.docportal.MetadataHistory"/>
        <mapping class="com.tcbs.automation.docportal.MetadataType"/>
        <mapping class="com.tcbs.automation.docportal.ObjectAttribute"/>
        <mapping class="com.tcbs.automation.docportal.ObjectType"/>
        <mapping class="com.tcbs.automation.docportal.PermissionType"/>
        <mapping class="com.tcbs.automation.docportal.ReferenceData"/>
        <mapping class="com.tcbs.automation.docportal.RolePermission"/>
        <mapping class="com.tcbs.automation.docportal.RoleType"/>
    </session-factory>
</hibernate-configuration>
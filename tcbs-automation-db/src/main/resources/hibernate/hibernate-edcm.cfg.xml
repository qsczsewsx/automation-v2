<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
        <property name="hibernate.dialect">org.hibernate.dialect.Oracle12cDialect</property>
        <property name="show_sql">true</property>
        <property name="hibernate.temp.use_jdbc_metadata_defaults">false</property>
        <property name="hibernate.proc.param_null_passing">true</property>

        <mapping class="com.tcbs.automation.edcm.cases.CaseInfo"/>
        <mapping class="com.tcbs.automation.edcm.cases.CaseDisbursementProgress"/>
        <mapping class="com.tcbs.automation.edcm.cases.CaseTeamMember"/>
        <mapping class="com.tcbs.automation.edcm.cases.CaseIssuanceParticipant"/>
        <mapping class="com.tcbs.automation.edcm.cases.CaseParticipantContact"/>
        <mapping class="com.tcbs.automation.edcm.cases.BondTempBaseInfo"/>
        <mapping class="com.tcbs.automation.edcm.cases.ViewBondTemplateSummary"/>
        <mapping class="com.tcbs.automation.edcm.cases.BondFees"/>
        <mapping class="com.tcbs.automation.edcm.cases.BondFeePaymentDetail"/>
        <mapping class="com.tcbs.automation.edcm.cases.BondBaseInfo"/>
        <mapping class="com.tcbs.automation.edcm.company.CompanyBaseInfo"/>

        <mapping class="com.tcbs.automation.edcm.bond.BondPrimaryDistribution"/>
        <mapping class="com.tcbs.automation.edcm.bond.BondListingTimeline"/>
        <mapping class="com.tcbs.automation.edcm.bond.BondToSell"/>
        <mapping class="com.tcbs.automation.edcm.bond.BondListingInfo"/>
        <mapping class="com.tcbs.automation.edcm.referencedata.ReferenceData"/>
    </session-factory>
</hibernate-configuration>